 <!-- start banner Area -->
<section class="banner-area relative" id="home">
    <div class="overlay overlay-bg"></div>
    <div class="container">
        <div class="row d-flex align-items-center justify-content-center">
            <div class="about-content col-lg-12">
                <h1 class="text-white">
                    Jobs Listing
                </h1>
                <p class="text-white link-nav"><a href="index.html">Home </a>  <span class="lnr lnr-arrow-right"></span>  <a href="category.html"> Job category</a></p>
            </div>
        </div>
    </div>
</section>
<!-- End banner Area -->
<section class="post-area section-gap">
    <div class="container">
        <div class="row justify-content-center d-flex">
            <div class="col-lg-8 post-list">
                <ul class="cat-list">
                    <li><a href="#">Recent</a></li>
                    <li><a href="#">Full Time</a></li>
                    <li><a href="#">Intern</a></li>
                    <li><a href="#">part Time</a></li>
                </ul>
@foreach (var item in Model)
{
    <div class="single-post d-flex flex-row">
        <div class="thumb">
            <img src="data:image/png;base64," alt="">
            <ul class="tags">
                <li>
                    @Html.ActionLink("Edit", "Edit", new { id=item.offerId })
                </li>
                <li>
                    @Html.ActionLink("Details", "Details", new { id = item.offerId })
                </li>
                <li>
                    @Html.ActionLink("Delete", "Delete", new { id = item.offerId })
                </li>
            </ul>
        </div>
        <div class="details">
            <div class="title d-flex flex-row justify-content-between">
                <div class="titles">
                    <h4>@Html.DisplayFor(modelItem => item.titleOffer)</h4>
                    <h6>@Html.DisplayFor(modelItem => item.contractTypeOffer)</h6>
                </div>
                <ul class="btns">
                    <li><a href="#"><span class="lnr lnr-heart"></span></a></li>
                    <li><a href="#">Apply</a></li>
                </ul>
            </div>
            <p>
                Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod temporinc ididunt ut dolore magna aliqua.
            </p>
            <h5></h5>
            <p class="address"><span class="lnr lnr-map"></span> @Html.DisplayFor(modelItem => item.location)</p>
            <p class="address"><span class="lnr lnr-database"></span> @Html.DisplayFor(modelItem => item.salary) $</p>
        </div>
    </div>
}
               
